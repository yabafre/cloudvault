{
	"extends": "@cloudvault/typescript-config/nestjs.json",
	"compilerOptions": {
		"outDir": "./dist",
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"],
			"@modules/*": ["src/modules/*"],
			"@common/*": ["src/common/*"],
			"@config/*": ["src/config/*"],
			"@prisma/*": ["src/prisma/*"],
			"@test/*": ["test/*"]
		}
	},
	"include": ["src/**/*.ts", "test/**/*.ts"]
}
